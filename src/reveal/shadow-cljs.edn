{:source-paths ["src"]
 :dependencies [[binaryage/devtools "1.0.3"]
                [reagent "1.1.0"]
                [nrepl "0.8.3"]
                [cider/cider-nrepl "0.24.0"]]
 :builds       {:app {:target     :browser
                      :output-dir "public/js"
                      :asset-path "/js"
                      :modules    {:app {:entries [reveal.core]}}
                      :devtools   {:after-load reveal.core/mount-root}}}

 :dev-http     {3000 {:root    "public"
                      :handler user/app}}}
